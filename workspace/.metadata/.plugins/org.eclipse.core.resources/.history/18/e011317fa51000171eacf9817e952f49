import java.util.ArrayList;

public abstract class Person {
	String Name;
	int index;
	ArrayList<Integer> prefList = new ArrayList<Integer>();
	Person Partner = null;
	
	public int getIndex() {
		return index;
	}
	
	public String getName() {
		return Name;
	}
	
	public ArrayList<Integer> getList() {
		return prefList;
	}
	
	public Person getPartner() {
		return Partner;
	}
}
